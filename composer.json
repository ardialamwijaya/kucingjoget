{
    "name": "yourname/yourproject",
    "description": "Project description",
    "type": "project",
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-pcre": "*",
        "symfony/polyfill-mbstring": "^1.7",
        "danog/madelineproto": "dev-master"
    },
    "archive": {
        "exclude": [
            "/build",
            "/doc",
            "/js",
            "/python",
            "/wiki",
            "/ccxt.wiki"
        ]
